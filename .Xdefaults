URxvt*geometry: 110x30
URxvt*depth: 32
URxvt*background: [84]#000000
URxvt*foreground: #bed6ff
URxvt*color12: #ffff00
URxvt*color4: #bbbbff
URxvt*scrollBar: false
URxvt*font: xft:Inconsolata:size=14
!URxvt*font: xft:nexus-font:size=18
URxvt*letterSpace: -1

xterm*geometry: 100x24
xterm*background: #060607
!xterm*foreground: #bbbbee
xterm*foreground: #99aaee
xterm*color12: #444477
xterm*color4: #9999ff
xterm*scrollbar: false
xterm*faceName: Tamsyn:size=16:antialias=true
!xterm*faceName: terminus:size=13:antialias=true
xterm*font: 80x30
xterm*termName: xterm-256color
xterm*utf8: 1

